pipeline {
    agent any
    tools{
maven 'maven3.8.2'

}
  stages{
   
    stage('CheckOutCode'){
    steps{
    git branch: 'master', credentialsId: 'GitHub', url: 'https://github.com/sushmareddymvsl/hello-world.git'
	
	}
  }
        stage('Build') { 
            	  steps {
                   sh 'mvn clean install'
		   }
        }
    }
}

pipeline {
    agent any
  stages{
   
    stage('CheckOutCode'){
    steps{
    git branch: 'master', credentialsId: 'GitHub', url: 'https://github.com/sushmareddymvsl/hello-world.git'
	
	}
  }
        stage('Build') {
            steps {
               agent { docker 'maven:3-alpine' } 
            	  steps {
                   sh 'mvn clean install'
            }
        }
    }
}
